{
  "name": "jhaas-frontend",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "config": {
    "ncuExcludePkgs": "eslint-plugin-vue"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint -c eslint.config.mjs src/",
    "format": "prettier --write src/",
    "update": "ncu -u"
  },
  "dependencies": {
    "marked": "^15.0.7",
    "pinia": "^3.0.1",
    "vee-validate": "^4.15.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@types/node": "^22.13.10",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.22.0",
    "eslint-plugin-vue": "^9.32.0",
    "npm-check-updates": "^17.1.15",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.1",
    "vite": "^6.2.2",
    "vue-tsc": "^2.2.8"
  }
}
